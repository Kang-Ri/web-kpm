            "name": "4. Orders Management",
            "item": [
                {
                    "name": "Create Order",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "pm.test(\"Order created successfully\", function () {",
                                    "    pm.response.to.have.status(201);",
                                    "    var jsonData = pm.response.json();",
                                    "    ",
                                    "    pm.expect(jsonData.data).to.have.property('idOrder');",
                                    "    pm.expect(jsonData.data).to.have.property('namaProduk');",
                                    "    pm.expect(jsonData.data).to.have.property('namaPembeli');",
                                    "    pm.expect(jsonData.data).to.have.property('emailPembeli');",
                                    "    pm.expect(jsonData.data).to.have.property('noHpPembeli');",
                                    "    pm.expect(jsonData.data).to.have.property('hargaFinal');",
                                    "    ",
                                    "    pm.environment.set(\"orderId\", jsonData.data.idOrder);",
                                    "    console.log(\"Order ID saved:\", jsonData.data.idOrder);",
                                    "    ",
                                    "    pm.expect(jsonData.data.namaPembeli).to.equal(\"Ahmad Rizki Pratama\");",
                                    "    pm.expect(jsonData.data.emailPembeli).to.equal(\"ahmad.rizki@email.com\");",
                                    "    pm.expect(jsonData.data.noHpPembeli).to.equal(\"081234567890\");",
                                    "    ",
                                    "    console.log(\"Snapshot verified:\", {",
                                    "        nama: jsonData.data.namaPembeli,",
